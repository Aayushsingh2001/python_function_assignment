   ],
   "source": [
    "tuples_list = [(1, 5), (2, 3), (4, 1), (3, 9)]\n",
    "\n",
    "sorted_list = sorted(tuples_list, key=lambda x: x[1])\n",
    "\n",
    "print(sorted_list)"
